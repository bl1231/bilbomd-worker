{
  "name": "bilbomd-worker",
  "version": "1.14.3",
  "description": "A worker app to process BilboMD jobs from BullMQ queue",
  "main": "worker.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "lint": "eslint",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src --ext ts,ejs --exec npm run start",
    "start": "npm run build && node dist/worker.js",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "Scott Classen",
  "license": "ISC",
  "dependencies": {
    "@bl1231/bilbomd-mongodb-schema": "^1.7.1",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "bullmq": "^5.53.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "glob": "^11.0.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.15.1",
    "nodemailer": "^7.0.3",
    "nodemailer-express-handlebars": "^7.0.0",
    "qs": "^6.14.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/express": "^5.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/shelljs": "^0.8.16",
    "@vitest/ui": "^3.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.28.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "shelljs": "^0.10.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.0"
  },
  "_moduleAliases": {
    "@services": "build/services",
    "@models": "build/models",
    "@helpers": "build/helpers"
  }
}
