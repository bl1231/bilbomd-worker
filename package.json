{
  "name": "bilbomd-worker",
  "version": "1.6.3",
  "description": "A worker app to process BilboMD jobs from BullMQ queue",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf build/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./build",
    "lint": "eslint . --ext .ts",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src --ext ts,ejs --exec npm run start",
    "start": "npm run build && node build/worker.js",
    "test": "NODE_ENV=test jest --testTimeout=5000 --runInBand --verbose --detectOpenHandles"
  },
  "keywords": [],
  "author": "Scott Classen",
  "license": "ISC",
  "dependencies": {
    "@bl1231/bilbomd-mongodb-schema": "^1.0.22",
    "axios": "^1.7.3",
    "axios-retry": "^4.4.2",
    "bullmq": "^5.12.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.5.2",
    "nodemailer": "^6.9.14",
    "nodemailer-express-handlebars": "^6.1.2",
    "qs": "^6.12.3",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.0.2",
    "@types/nodemailer": "^6.4.15",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "_moduleAliases": {
    "@services": "build/services",
    "@models": "build/models",
    "@helpers": "build/helpers"
  }
}
